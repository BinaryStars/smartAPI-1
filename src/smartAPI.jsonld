{
  "@context": {
    "@vocab": "http://example.org/",
    "dcat": "http://www.w3.org/ns/dcat#",
    "dct": "http://purl.org/dc/terms/",
    "foaf": "http://xmlns.com/foaf/0.1/",
    "ids": "http://identifiers.org/",
    "pav": "http://purl.org/pav/version",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "schema": "http://schema.org/",
    "sio": "http://semanticscience.org/resource/",
    "xsd": "http://www.w3.org/2001/XMLSchema#",

    "_id": "@id",

    "label": {
      "@id": "rdfs:label",
      "@type": "xsd:string",
      "@language": "en"
    },
    "min": {
      "@id": "minimum-cardinality",
      "@type": "xsd:integer"
    },
    "max": {
      "@id": "maximum-cardinality",
      "@type": "xsd:integer"
    },
    "name": {
      "@id": "dct:title",
      "@type": "xsd:string",
      "label": "name",
      "description": "The name of the API",
      "@language": "en",
      "min": "1",
      "max": "1"
    },
    "description": {
      "@id": "dct:description",
      "@type": "xsd:string",
      "label": "description",
      "description": "A human readable description of the API",
      "@language": "en",
      "min": "0",
      "max": "1"
    },
    "version": {
      "@id": "pav:version",
      "@type": "xsd:string",
      "label": "version",
      "description": "An API version number, preferably as major.minor.patch",
      "min": "0",
      "max": "1"
    },
    "logo": {
      "@id": "schema:logo",
      "@type": "@id",
      "label": "logo",
      "description": "A picture depicting the API",
      "min": "0"
    },
    "publisher": {
      "@id": "dct:publisher",
      "@type": "Publisher",
      "label": "publisher",
      "description": "The URI for the publisher.",
      "min": "1"
    },
    "Publisher": {
      "@id": "foaf:Agent",
      "@type": "@id",
      "label": "Publisher",
      "description": "Publisher of the API",
      "name": {
        "@id": "dct:title",
        "@type": "xsd:string",
        "label": "The name of the publisher",
        "min":"0",
        "max":"1"
      }
    },
    "homepage": {
      "@id": "foaf:page",
      "@type": "@id",
      "label": "logo",
      "description": "A picture depicting the API",
      "min": "1"
    },
    "documentionPage": {
      "@id": "documentation",
      "@type": "@id",
      "label": "documentation",
      "description": "Documentation for the API",
      "min": "0"
    },
    "termsOfUsePage": {
      "@id": "termsOfUsePage",
      "@type": "@id",
      "label": "Terms of use page",
      "description": "Document describing the API terms of the service",
      "min": "1"
    },
    "sourceCodeURL": {
      "@id": "sourceCodeURL",
      "@type": "@id",
      "label": "source code URL",
      "description": "The location for the API source code",
      "min": "0"
    },
    "publication": {
      "@id": "ids:pubmed/",
      "@type": "@id"
    },
    "keywords": {
      "@id": "dcat:keyword",
      "@type": "xsd:string",
      "label": "keyword",
      "description": "Terms that describe the API",
      "min": "1"
    },
    "services": {
      "@id": "sio:contains",
      "@type": "@id",
	  "label": "API services",
	  "description": "Services that the API provides",
	  "min":"1"
    }
  }
}
